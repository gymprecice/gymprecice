FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
libs ("libCustomisedMeshTools.so" "libCustomisedBCs.so");

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         2;

deltaT          5e-4;

//writeControl    runTime;
//writeInterval   5e-2;

writeControl   timeStep; //adjustableRunTime;
writeInterval   1;

purgeWrite      1;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable false;

adjustTimeStep  no;
maxCo           1;
functions
{
    #includeFunc solverInfo
    #includeFunc forces
    #includeFunc probes
    #includeFunc flowRateJet1
    #includeFunc flowRateJet2
// #includeFunc preCICE_Adapter        
}


