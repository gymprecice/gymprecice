FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime    0.005;

deltaT          0.0005;

writeControl   timeStep;

writeInterval   1;

purgeWrite      1;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

adjustTimeStep  no;

maxCo           1;

functions
{
    #includeFunc forces
    #includeFunc probes
    #includeFunc flowRateJet1
    #includeFunc flowRateJet2
// #includeFunc preCICE_Adapter        
}


